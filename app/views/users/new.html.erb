<div id="content">
  <%= error_messages_for :user %>
  <% form_for :user,:url => users_path do |f|%>
    <h4>Sing Up</h4>
    <ul>
      <label>Username:</label><br/>
      <%= f.text_field :username, :class => 'input'%><p/>
      <label>Nike</label><br/>
      <%= f.text_field :nike, :class => 'input'%><p/>
      <label>Password:</label><br/>
      <%= f.password_field :password, :class => 'input'%><p/>
      <label>Password Confirmation:</label><br/>
      <%= f.password_field :password_confirmation, :class => 'input'%><p/>
      <label>Email </label><br/>
      <%= f.text_field :email, :class => 'input' %><p/>
      <%= submit_tag 'Sign Up', :class => 'button'%> | <%= link_to 'Cancel', home_path %>
    </ul>
  <% end %>
</div>
